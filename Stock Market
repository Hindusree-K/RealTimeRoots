#include <stdio.h>
#include <string.h>

int main() {
   int arr[5];
   int max=0,start=0,end=0;
   
   printf("Enter the array elements");
   for(int i=0;i<5;i++){
       scanf("%d",&arr[i]);
   }
   for(int i=0;i<4;i++){
       for(int j=i+1;j<5;j++){
           if(arr[i]<arr[j]&&arr[j]-arr[i]>max){
               max=arr[j]-arr[i];
               start=i+1;
               end=j+1;
           }
       }
   }
 printf("The maximum profit is %d is earned when we buy on %d,and sell on %d",max,start,end);
    return 0;
}
