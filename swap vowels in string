#include <stdio.h>
#include<string.h>

int main() 
{
    char str[]="helloworld";
    char temp;
    int n=strlen(str);
    int i,j;
    for(i=0;i<n;i++)
    {
        for(j=n;j>=0;j--)
        {
            
            if(str[i]=='a'||str[i]=='e'||str[i]=='i'||str[i]=='o'||str[i]=='u'||str[i]=='A'||str[i]=='E'||str[i]=='O'||str[i]=='I'||str[i]=='U'){
            if(str[j]=='a'||str[j]=='e'||str[j]=='i'||str[j]=='o'||str[j]=='u'||str[j]=='A'||str[j]=='E'||str[j]=='O'||str[j]=='I'||str[j]=='U')
                {
                    temp=str[i];
                    str[i]=str[j];
                    str[j]=temp;
                    
                    
                }
            }
        }
    }
    printf("%s",str);

    return 0;
}
